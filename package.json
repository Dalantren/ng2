{
  "name": "ng2-qgrid",
  "version": "9.2.1",
  "scripts": {
    "build:app": "npm run clean && ng build --prod --output-path dist/app --base-href /ng2/",
    "build:lib": "npm run clean && node build.lib.js",
    "clean": "rimraf dist",
    "lint": "ng lint",
    "ng": "ng",
    "preinstall": "npm i glob && node preinstall.js",
    "postinstall": "npm run clean",
    "pretest:screenshot": "webdriver-manager update --gecko=false && if not exist e2e\\diff mkdir e2e\\diff",
    "release:app": "npm run build:app && npx angular-cli-ghpages --dir=dist/app",
    "release:example": "node ./build.example",
    "release:lib": "npm run build:lib && cd dist/ng2-qgrid && npm publish",
    "start:prod": "node build.serve --prod",
    "start": "node build.serve",
    "test:core": "karma start ./projects/qgrid-core/karma.conf.js --single-run",
    "test:core-watch": "karma start ./projects/qgrid-core/karma.conf.js ",
    "test:screenshot-update-goldens": "protractor ./e2e/protractor-update-goldens.conf.js",
    "test:screenshot": "protractor ./e2e/protractor.conf.js"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~9.1.0",
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/core": "^9.1.0",
    "@angular/forms": "~9.1.0",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "@qgrid/core": "file:./projects/qgrid-core",
    "@qgrid/ngx": "file:./dist/ngx-qgrid",
    "@qgrid/ngx-plugins": "file:./dist/ngx-qgrid-plugins",
    "@qgrid/plugins": "file:./projects/qgrid-plugins",
    "core-js": "^3.6.4",
    "rxjs": "~6.5.4",
    "tslib": "^1.11.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.901.0",
    "@angular-devkit/build-ng-packagr": "0.901.0",
    "@angular/cdk": "^9.2.1",
    "@angular/cli": "^9.1.0",
    "@angular/compiler-cli": "~9.1.0",
    "@angular/language-service": "~9.1.0",
    "@angular/material": "^9.2.1",
    "@angular/router": "~9.1.0",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/cucumber": "^6.0.1",
    "@types/lodash-es": "^4.17.3",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.9.8",
    "angular-cli-ghpages": "^0.6.2",
    "babel-loader": "^8.2.1",
    "blue-harvest": "^0.3.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-spies": "^1.0.0",
    "classlist.js": "^1.1.20150312",
    "codelyzer": "^5.2.2",
    "command-line-args": "^5.1.1",
    "css.escape": "^1.5.1",
    "cucumber": "^6.0.5",
    "fastdom": "^1.0.9",
    "file-saver": "^2.0.2",
    "glob": "^7.1.6",
    "karma": "~4.4.1",
    "karma-chai": "^0.1.0",
    "karma-chai-spies": "^0.1.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-webpack": "^4.0.2",
    "livr": "2.2.5",
    "lodash-es": "^4.17.15",
    "mocha": "^7.1.1",
    "ng-packagr": "^9.1.0",
    "npm-sass": "^2.3.0",
    "protractor": "~5.4.3",
    "protractor-cucumber-framework": "^6.2.1",
    "sane": "^4.1.0",
    "shelljs": "^0.8.3",
    "ts-node": "~8.8.1",
    "tsickle": "^0.38.1",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3",
    "webpack": "^4.44.2",
    "xlsx": "^0.15.6"
  }
}
